<!-- Modal -->
<div class="modal fade" id="popUpApagar" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content p-3">
            <form  id="formApagar"  method="post">
                <input type="hidden" name="id" id="idParaExcluir" />

                <div class="modal-header">
                    <h5 class="modal-title" id="texto_popup"></h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                </div>

                <div class="modal-body">
                    <h3 id="input_texto_contato"></h3>
                </div>

                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Sim</button>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancelar</button>
                </div>
            </form>
        </div>
    </div>
</div>
